{% extends "TecServiceBundle::layout.html.twig" %}
{% block title %}Populaide - Belgique - Results{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('css/show-pages.css') }}" rel="stylesheet" type="text/css" />
{% endblock %}
{% block body %}
    {{ parent() }}
     <section id="works">
        Types:
        <ul>
        {% for type in types %}
            <li>{{ type.id ~ " " ~ type.intitule }}<a href="{{ path('tec_service_updatetype',{'id': type.id}) }}"> Modifier</a><a href="{{ path('tec_service_delcategorie',{'id': type.id}) }}"> Supprimer</a></li>
                <ul>
                {% for annonce in type.getAnnonces %}
                    <li>{{ annonce.id ~ " " ~ annonce.title }}</li>
                {% endfor %}
                </ul>
        {% endfor %}
        </ul>
    </section>
    
{% endblock %}
