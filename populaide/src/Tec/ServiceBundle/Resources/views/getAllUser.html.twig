{% extends "TecServiceBundle::layout.html.twig" %}
{% block title %}Populaide - Belgique - Results{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('css/show-pages.css') }}" rel="stylesheet" type="text/css" />
{% endblock %}
{% block body %}
    {{ parent() }}
     <section id="works">
        Users:
        <ul>
        {% for user in users%}
            <li>{{ user.id ~ " " ~ user.username ~ " " ~ user.email }}<a href="{{ path('tec_service_updateuser',{'id': user.id}) }}"> Modifier</a><a href="{{ path('tec_service_deluser',{'id': user.id}) }}"> Supprimer</a></li>
        {% endfor %}
        </ul>
    </section>
    


{% endblock %}
